  "description": "A website created with ReactJS, Alt, and the Bootstrap CSS framework.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-js && npm run build-css",
    "build-css": "node-sass assets/scss/app.scss build/css/app.css",
