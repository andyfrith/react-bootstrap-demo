  "dependencies": {
    "alt": "^0.16.0",
    "bootstrap-sass": "^3.3.6",
    "browserify": "^13.0.0",
    "flux": "^2.0.1",
    "node-sass": "^3.4.2",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.6",
    "reactify": "^1.1.1"
  }
}
